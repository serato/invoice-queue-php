{
    "name": "serato/sqs-invoice-queue",
    "homepage": "https://github.com/serato/invoice-queue-php",
    "description": "A library for interacting with an SQS message queue that holds invoice data.",
    "type": "library",
    "require": {
        "php": "^7.1 || ^8.2",
		"aws/aws-sdk-php": "^3.0.0",
        "justinrainbow/json-schema": "^5.0.0",
        "psr/log": "^1.0.0",
        "monolog/monolog": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7 || ^8",
        "phpstan/phpstan": "^1.4",
        "squizlabs/php_codesniffer": "^3",
        "enlightn/security-checker": "^1.4 || ^2"
    },
    "autoload": {
        "psr-4": {
            "Serato\\InvoiceQueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Serato\\InvoiceQueue\\Test\\": "tests/"
        }
    }
}